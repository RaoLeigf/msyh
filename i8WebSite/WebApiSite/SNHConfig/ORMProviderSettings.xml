    <property name="['NG0001']" ref="NG0001DbProvider" />
    <property name="['NG0003']" ref="NG0003DbProvider" />
    <property name="['NG0007']" ref="NG0007DbProvider" />
    <property name="['NGSoft']" ref="NGSoftDbProvider" />
    <property name="['NGLog']" ref="NGLogDbProvider" />
  </object>
  <db:provider id="DefaultDbProvider" provider="${provider}" connectionString="${defaultConnection}" />
  <db:provider id="NG0001DbProvider" provider="${provider}" connectionString="${NG0001Connection}" />
  <db:provider id="NG0003DbProvider" provider="${provider}" connectionString="${NG0003Connection}" />
  <db:provider id="NG0007DbProvider" provider="${provider}" connectionString="${NG0007Connection}" />
  <db:provider id="NGSoftDbProvider" provider="${provider}" connectionString="${NGSoftConnection}" />
  <db:provider id="NGLogDbProvider" provider="${provider}" connectionString="${NGLogConnection}" />
</objects>