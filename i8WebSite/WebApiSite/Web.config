    <add key="Language" value="zh-CN" />
    <add key="UseWebGarden" value="0" />
    <add key="SateCacheURL" value="tcp://localhost:36988/CacheServer" />
    <add key="MessageCacheURL" value="tcp://localhost:36987/CacheServer" />
