    <add key="IsCheckMaxRecord" value="true" />
    <!--可选，系统是否检查获取最大记录数；默认启用且与AllowMaxRecords有关-->
    <add key="AllowMaxRecords" value="500" />
    <!--可选，启用最大记录数检查，默认500-->
    <add key="AppKey" value="D31B7F91-3068-4A49-91EE-F3E13AE5C48C" />
