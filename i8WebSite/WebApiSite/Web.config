    <!--api地址：http://127.0.0.1:8028/api/GCW/SysToken/GetToken?token=g6c  http://localhost/custom/api/GCW/SysToken/GetToken?token=g6c -->
    <add key="apiurl" value="http://localhost/custom/api/GCW/SysToken/GetToken?token=g6c"/>
    <!--IIS程序池心跳激活时间间隔(分钟)-->
