<?xml version="1.0" encoding="utf-8"?>
<nhv-mapping namespace="SUP3.Log.Model.Domain" assembly="SUP3.Log.Model" xmlns="urn:nhibernate-validator-1.0">
	<class name="LogSqlModel">
		<property name="Phid" >
			<not-null />
			<not-Empty />
		</property>
		<property name="LogId" >
			<length min="0" max="20" message ="长度不超过20！"/>
		</property>
		<property name="SessionId" >
			<length min="0" max="40" message ="长度不超过40！"/>
		</property>
		<property name="Url" >
			<length min="0" max="100" message ="长度不超过100！"/>
		</property>
		<property name="SqlText" >
			<length min="0" max="4000" message ="长度不超过4000！"/>
		</property>
		<property name="ClientIp" >
			<length min="0" max="20" message ="长度不超过20！"/>
		</property>
		<property name="CollectType" >
			<length min="0" max="10" message ="长度不超过10！"/>
		</property>
	</class>
</nhv-mapping>
