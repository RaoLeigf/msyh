                    if (data_0.FWeight + data_1.FWeight > 100) {
                        Ext.MessageBox.alert("提示", "权重之和大于100,请重新分配");

                        if (row === 0) {
                            data_0.FWeight = 100 - data_1.FWeight;
                            Ext.apply(thisStore.getAt(0).data, data_0);
                            this.getView().refresh(true);

