            function GetComboboxItemCount(target, q) {

                var data = $.data(target, "combobox").data;
                var opts = $(target).combobox("options");
                var count = 0;
                if (data != "" && data != null && data != undefined) {
                    for (var i = 0; i < data.length; i++) {
